#include <iostream>
using namespace std;
int convertingseconds(int s,int &h,int &m) {
  h=s/3600;
  s=s%3600;
  m=s/60;
  s=s%60;
 return s;   
}
int main() {
  int s,h,m;
  cout<<"Insert Seconds = ";
  cin>>s;
int secondsleft=convertingseconds(s,h,m);
  cout<<"Hours = "<<h<<endl;
  cout<<"Minutes = "<<m<<endl;
  cout<<"Seconds = "<<secondsleft;
 return 0;
}
