#include <iostream>
using namespace std;
int threedigitssumtoten(int a,int &s,int &k) {
  int n=999;
for (int i=100;i<=n;i++) {
  a=i;
  s=0;
while(a>0) {
  k=a%10;
  s=s+k;
  a=a/10;
}
if (s==10) {
  cout<<i<<" ";
}
}
  return s; 
}
int main() {
  int a,s,k;
  int result=threedigitssumtoten(a,s,k);
 return 0;
}
